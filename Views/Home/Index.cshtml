@model List<codingskills.Models.Language>


<div class="jumbotron">
<h2 class="text-center">DotNetCore - MVC App</h2>
</div>

<div class="well">

<h4 class="text-center"> My Code Polyglot List:</h4>

<form asp-action="Create" asp-controller="Home">
  <input name="LanguageName" />
  <input class="btn btn-default" type="submit" value="Add Character"/>
</form>


  <ul>
  @foreach (var language in Model)
  {
  <li>@language.Name</li>
  }
  </ul>
</div>
